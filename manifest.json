{
  "manifest_version": 2,
  "name": "my-extension",
  "description": "prints hello world",
  "version": "1.0.0",
  "icons": { "128": "icon_128.png" },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["popup.js"]
    }
  ],
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "background",
    "http://*/*",
    "https://*/*",
    "tabs"
  ]
}
